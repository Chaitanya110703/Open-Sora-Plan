{
    "ae": {
        "model_id": "cogvideox",
        "from_pretrained": "CogVideoX-5b/vae",
        "dtype": "float16"
    },
    "text_encoder": {
        "hub_backend": "hf",
        "model_id": "T5",
        "from_pretrained": "CogVideoX-5b/text_encoder",
        "dtype": "float16"
    },
    "tokenizer":{
        "hub_backend": "hf",
        "autotokenizer_name": "T5Tokenizer",
        "from_pretrained": "CogVideoX-5b/tokenizer"
    },
    "predictor": {
        "dtype": "float16",
        "model_id": "cogvideox",
        "model_version": "cogvideox_5b",
        "from_pretrained": "CogVideoX-5b/transformer"
    },
    "diffusion": {
        "model_id": "cogvideox",
        "model_version": "cogvideox_5b",
        "from_pretrained": "CogVideoX-5b/scheduler",
        "clip_sample": false,
        "prediction_type": "v_prediction",
        "rescale_betas_zero_snr": true,
        "timestep_spacing": "trailing"
    },

    "pipeline_config": {
        "use_dynamic_cfg": true,
        "seed": 42,
        "input_size": [49, 480, 720],
        "guidance_scale": 6.0,
        "num_inference_steps": 50
    },

    "frame_interval": 1,
    "micro_batch_size": 1,
    "model_max_length": 226,
    "save_path":"./cogvideox_samples/",
    "fps": 8,
    "prompt":"examples/cogvideox/samples_prompts.txt",
    "pipeline_class": "CogVideoXPipeline",
    "device":"npu",
    "dtype": "float16"
}